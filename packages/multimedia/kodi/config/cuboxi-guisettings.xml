<settings>
  <locale>
    <timezone>TZ</timezone>
    <timezonecountry>TZ_COUNTRY</timezonecountry>
  </locale>

  <services>
    <airplay default="true">false</airplay>
    <upnpannounce default="true">true</upnpannounce>
    <upnpcontroller default="true">false</upnpcontroller>
    <upnpserver default="true">false</upnpserver>
    <upnprenderer>true</upnprenderer>
    <webserver>true</webserver>
    <zeroconf default="true">true</zeroconf>
  </services>

  <lookandfeel>
    <enablerssfeeds>false</enablerssfeeds>
    <font>Default</font>
    <rssedit></rssedit>
    <skincolors>SKINDEFAULT</skincolors>
    <skintheme>SKINDEFAULT</skintheme>
    <skinzoom>0</skinzoom>
    <soundskin>OFF</soundskin>
    <startupwindow>10000</startupwindow>
  </lookandfeel>

  <audiooutput>
    <ac3passthrough>true</ac3passthrough>
    <audiodevice>ALSA:hdmi:CARD=imxhdmisoc,DEV=0</audiodevice>
    <channels>1</channels>
    <config>2</config>
    <dtshdpassthrough>false</dtshdpassthrough>
    <dtspassthrough>true</dtspassthrough>
    <eac3passthrough>false</eac3passthrough>
    <guisoundmode>1</guisoundmode>
    <normalizelevels>true</normalizelevels>
    <passthrough>false</passthrough>
    <passthroughdevice>ALSA:hdmi:CARD=imxhdmisoc,DEV=0</passthroughdevice>
    <processquality>50</processquality>
    <samplerate>48000</samplerate>
    <stereoupmix>false</stereoupmix>
    <streamsilence>1</streamsilence>
    <truehdpassthrough>false</truehdpassthrough>
  </audiooutput>

  <videoscreen>
    <delayrefreshchange>0</delayrefreshchange>
  </videoscreen>

</settings>
